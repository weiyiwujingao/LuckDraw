<script>
function SendTel(){
	var Tel = C.G('Phone').value;
	if(Tel==''){
		alert('请填写电话号码');
		return;
	}
	$.ajax({
		url: '<?=base_url()?>phone.html',
		cache: false,
		type: 'post',
		dataType: 'json',
		data: {phone:Tel,username:"<?=$username?>",key:"<?=$key?>",t:<?=$time?>},
		success: function(data){
			Dialog.Close();
		},
		error: function(XMLHttpRequest, textStatus, errorThrown){
			alert('网络繁忙，请重新提交！');
			return false;
		}
	});
}
//抽奖
$(function(){
	var $souceurl = "<?=base_url()?>newgifts.html";
	if($.cookie('a_k')){
		var IsLogin= 1;
	}else{
		var IsLogin=0;
	}
	var rotateFunc = function(awards,angle,text){
		$('#D10_8').stopRotate();
		$("#D10_8").rotate({
			angle:0, 
			duration:5000, 
			animateTo:angle+3600,
			callback:function(){
				if(awards==2 || awards==6 || awards==8){
					Dialog('WnBg','Dia4');
					C.G('Nums').innerHTML=text;
				}else if(awards==3){
					Dialog('WnBg','Dia2');
				}else if(awards==4 || awards==5){
					Dialog('WnBg','Dia1');
				}else if(awards==7){
					Dialog('WnBg','Dia3');
				}else{
					alert('系统错误，请稍后再试');
				}
			}
		});
	};
	$("#D10_8").rotate({
		bind:{
			click: function(){
				if(IsLogin==0){
					Dialog('WnBg','Login');
					return;
				}
				$.ajax({
					url: $souceurl,
					cache: false,
					type: 'post',
					dataType: 'json',
					data: {username:"<?=$username?>",userid:"<?=$userid?>",key:"<?=$key?>",t:<?=$time?>},
					success: function(jsondata){
						var data = [1,2,3,4,5,6,7,8];
							data = data[jsondata.flag];
						switch(jsondata.flag){
							case '1':
								rotateFunc(1,0,'iPhone6s');
							break;
							case '2':
								rotateFunc(2,45,'25元红包');
							break;
							case '3':
								rotateFunc(3,90,'谢谢参与');
							break;
							case '4':
								rotateFunc(4,135,'10元话费');
							break;
							case '5':
								rotateFunc(5,180,'精美相框');
							break;
							case '6':
								rotateFunc(6,225,'100元红包');
							break;
							case '7':
								Dialog('WnBg','Dia3');
							break;
							case '8':
								rotateFunc(8,315,'50元红包');
							break;
							default:
								
							break;
						}
					},
					error: function(XMLHttpRequest, textStatus, errorThrown){
						alert('网络繁忙，请稍后再试！');
						return false;
					}
				});
			}
		}
	});
});

</script>
